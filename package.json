{
  "name": "discord-ssh",
  "version": "4.0.0",
  "description": "Discord bot for using shell commands remotely through Discord",
  "type": "module",
  "scripts": {
    "start": "tsx ./index.ts",
    "dev": "tsx watch ./index.ts",
    "format": "prettier . --write --ignore-unknown --cache",
    "format:check": "prettier . --check --cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "chalk": "5.4.1",
    "discord.js": "14.21.0",
    "dotenv": "17.0.1",
    "strip-ansi": "7.1.0",
    "systeminformation": "5.27.7"
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "3.1.0",
    "@igorkowalczyk/prettier-config": "3.1.0",
    "@types/node": "24.0.8",
    "eslint": "9.30.1",
    "jiti": "2.4.2",
    "prettier": "3.6.2",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IgorKowalczyk/discord-ssh.git"
  },
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IgorKowalczyk/discord-ssh/issues"
  },
  "homepage": "https://github.com/IgorKowalczyk/discord-ssh#readme",
  "keywords": [
    "discord",
    "node-js",
    "discord",
    "discord-ssh",
    "ssh",
    "remote-connection"
  ],
  "packageManager": "pnpm@10.12.4",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
